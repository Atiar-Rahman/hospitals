{
    "builds": [
        {
            "src": "hospital_management/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "functions": {
        "hospital_management/wsgi.py": {
            "runtime": "python3.11"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "hospital_management/wsgi.py"
        }
    ]
}