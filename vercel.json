{
    "builds": [
        {
            "src": "hospital_management/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.12"
            }
        }
    ],
    "routes": [
        {
            "src": "hospital_management/(.*)",
            "dest": "/wsgi.py"
        }
    ]
}